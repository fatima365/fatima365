from pymongo import MongoClient

# Replace the uri string with your MongoDB deployment's connection string.
client = MongoClient("mongodb+srv://fatima:fatima@fatimacluster.xym4i.mongodb.net/")

# Select the database
db = client["demo"]

# Select the collection
collection = db["price"]

 # Data to be inserted
data = {
    "name" : "Fatima",
    "father name" : "M.Arshad",
    "age" : 18,
    "city" : "Faisalabad",
    "class" : "DDM",
    "roll no" : 2433,
    "semster" : "1st",
    "country" : "pakistan",
}

result = collection.insert_one(data)
print("Data inserted with ID:", result.inserted_id)
